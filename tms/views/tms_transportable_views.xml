<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="tms_transportable_view_tree" model="ir.ui.view">
        <field name="name">tms.transportable.tree</field>
        <field name="model">tms.transportable</field>
        <field name="arch" type="xml">
            <tree editable="bottom" string="Transportable">
                <field name="name"/>
                <field name="uom_id" options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True}"/>
            </tree>
        </field>
    </record>

    <record id="tms_transportable_action" model="ir.actions.act_window">
        <field name="name">Transportable</field>
        <field name="res_model">tms.transportable</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>
