<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="tms_custom_house_view_tree" model="ir.ui.view">
        <field name="name">Customs House</field>
        <field name="model">tms.custom.house</field>
        <field name="arch" type="xml">
            <tree string="Description" editable="bottom">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="tms_custom_house_view_form" model="ir.ui.view">
        <field name="name">Custom House</field>
        <field name="model">tms.custom.house</field>
        <field name="arch" type="xml">
            <form string="Description">
                <sheet>
                <group>
                    <field name="name"/>
                </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="tms_custom_house_action" model="ir.actions.act_window">
        <field name="name">Custom House</field>
        <field name="res_model">tms.custom.house</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
