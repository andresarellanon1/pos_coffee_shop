<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="fleet_vehicle_engine_view_tree" model="ir.ui.view" >
        <field name="name">fleet_vehicle_engine.tree</field>
        <field name="model">fleet.vehicle.engine</field>
        <field name="arch" type="xml">
            <tree string="Engines" editable="top">
                <field name="name"/>
            </tree>
        </field>
    </record>

    <record id="fleet_vehicle_engine_tms_action" model="ir.actions.act_window">
        <field name="name">Engines</field>
        <field name="res_model">fleet.vehicle.engine</field>
        <field name="view_mode">tree</field>
    </record>
</odoo>
