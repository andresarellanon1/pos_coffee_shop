<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="tms_waybill_view_tree" model="ir.ui.view">
        <field name="name">tms.waybill.tree</field>
        <field name="model">tms.waybill</field>
        <field name="priority">2</field>
        <field name="arch" type="xml">
            <tree string="Waybills" decoration-success="state == 'approved'" decoration-danger="state == 'draft'"
                decoration-muted="state == 'cancel'">
                <field name="name"/>
                <field name="date_order"/>
                <field name="partner_id"/>
                <field name="amount_untaxed" sum="Amount Untaxed"/>
                <field name="amount_tax"/>
                <field name="amount_total"/>
                <field invisible="1" name="invoice_id"/>
                <field invisible="1" name="user_id"/>
                <field name="state"/>
                <button icon="fa-thumbs-up" name="action_approve" states="draft" string="Approve" type="object"/>
                <button icon="fa-check-square-o" name="action_confirm" states="approved" string="Confirm" type="object"/>
            </tree>
        </field>
    </record>

    <record id="view_tms_waybill_form" model="ir.ui.view">
        <field name="name">tms.waybill.form</field>
        <field name="model">tms.waybill</field>
        <field name="arch" type="xml">
            <form string="Waybill">
                <header>
                    <button name="action_cancel" icon="fa-times" states="draft,approved,confirmed" string="Cancel" type="object"/>
                    <button name="action_cancel_draft"  icon="fa-reply" states="cancel" string="Set to Draft" type="object"/>
                    <button name="action_approve"  class="oe_highlight" icon="fa-thumbs-up" states="draft" string="Approve" type="object"/>
                    <button name="action_confirm" class="oe_highlight" icon="fa-check-square-o" states="approved" string="Confirm" type="object"/>
                    <field name="state" statusbar_visible="draft,approved,confirmed" widget="statusbar"/>
                </header>
                <sheet>
                    <h1>
                        <field name="name" class="oe_inline" readonly="1"/>
                    </h1>
                    <group>
                        <group>
                            <field name="operating_unit_id" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"
                                options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True}"/>
                            <field name="currency_id" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}" groups="base.group_multi_currency"
                                options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True}"/>
                        </group>
                        <group>
                            <field attrs="{'readonly':[('state', 'in', ('confirmed', 'cancel'))]}" name="date_order"/>
                        </group>
                    </group>
                    <separator colspan="2" string="·  ·  ·  ·  ·  ·  ·  ·  ·  ·  ·  ·  ·  ·  ·  ·" style="text-align: center;"/>
                    <group colspan="2">
                        <field name="partner_id" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}" colspan="2"
                            domain="[('is_company', '=', True)]"
                            options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True}"/>
                    </group>
                    <group>
                        <group>
                            <field name="partner_order_id" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"
                                context="{'search_default_customer': 1, 'show_address': 1}"
                                domain="[('parent_id', '=', partner_id)]"
                                options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True, 'always_reload': True}"/>
                            <field name="departure_address_id"  attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"
                                context="{'search_default_customer': 1, 'show_address': 1}"
                                options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True, 'always_reload': True}"/>
                            <field name="upload_point" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                        </group>
                        <group>
                            <field name="partner_invoice_id"  attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"
                                context="{'search_default_customer': 1, 'show_address': 1}"
                                domain="[('parent_id', '=', partner_id)]"
                                options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True, 'always_reload': True}"/>
                            <field name="arrival_address_id" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"
                                context="{'search_default_customer': 1, 'show_address': 1}"
                                options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True, 'always_reload': True}"/>
                            <field name="download_point" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}" />
                        </group>
                    </group>
                    <separator/>
                    <notebook colspan="4">
                        <page string="Lines">
                            <group>
                                <separator coslpan="4" string="Travels"/>
                                <field name="travel_ids" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}" colspan="4"
                                    domain="[('state', 'not in', ('cancel', 'closed')), ('operating_unit_id', '=', operating_unit_id)]" nolabel="1"/>
                                <separator coslpan="4" string="Waybill Factor Client"/>
                                <field name="customer_factor_ids" attrs="{'readonly':[('state', 'in', ('confirmed', 'cancel'))]}" colspan="4"
                                    context="{'default_category': 'customer'}" nolabel="1"/>
                                <separator coslpan="4" string="Transportable"/>
                                <field name="transportable_line_ids" attrs="{'readonly':[('state', 'in', ('confirmed', 'cancel'))]}" colspan="4" nolabel="1">
                                    <tree editable="bottom">
                                        <field name="transportable_id"
                                            options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True}"/>
                                        <field name="name"/>
                                        <field name="transportable_uom_id"
                                            options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True}"/>
                                        <field name="quantity"/>
                                        <field name="notes"/>
                                    </tree>
                                </field>
                                <separator colspan="4" string="Waybill Lines"/>
                                <field name="waybill_line_ids" attrs="{'readonly':[('state','in',('confirmed', 'cancel'))]}" colspan="4" nolabel="1">
                                    <tree editable="bottom" string="Waybill Lines">
                                        <field name="product_id" domain="[('tms_product_category', 'in', ('freight', 'move', 'insurance', 'tolls', 'other'))]"
                                            options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True}"/>
                                        <field name="name"/>
                                        <field name="product_qty"/>
                                        <field name="unit_price"/>
                                        <field name="discount"/>
                                        <field name="tax_ids" widget="many2many_tags"/>
                                        <field name="price_subtotal"/>
                                        <field name="tax_amount" invisible="1"/>
                                    </tree>
                                </field>
                                <group col="4" colspan="4">
                                    <group colspan="2">
                                        <field name="tax_line_ids" attrs="{'readonly': [('state', 'in', ('confirmed'))]}" nolabel="1" colspan="2">
                                            <tree create="false" delete="false">
                                                <field name="tax_id" options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True}"
                                                    force_save="1"/>
                                                <field name="base"/>
                                                <field name="tax_amount" sum="Impuestos"/>
                                            </tree>
                                        </field>
                                    </group>
                                    <group name="sale_total" class="oe_subtotal_footer oe_right" colspan="2">
                                        <field name="amount_freight" options="{'currency_field': 'currency_id'}" widget="monetary"/>
                                        <field name="amount_move" options="{'currency_field': 'currency_id'}" widget="monetary"/>
                                        <field name="amount_highway_tolls" options="{'currency_field': 'currency_id'}" widget="monetary"/>
                                        <field name="amount_insurance" options="{'currency_field': 'currency_id'}" widget="monetary"/>
                                        <field name="amount_other" options="{'currency_field': 'currency_id'}" widget="monetary"/>
                                        <field name="amount_untaxed" class="oe_subtotal_footer_separator" options="{'currency_field': 'currency_id'}" widget="monetary"/>
                                        <field name="amount_tax" options="{'currency_field': 'currency_id'}" widget="monetary"/>
                                        <field name="amount_total" class="oe_subtotal_footer_separator oe_inline" options="{'currency_field': 'currency_id'}" widget="monetary" colspan="2"/>
                                    </group>
                                </group>
                            </group>
                        </page>
                        <page string="Extra Fields">
                            <group col="4" colspan="4">
                                <newline/>
                                <field name="waybill_extradata_ids" context="{'default_waybill_id': active_id, 'active_model_base': active_model}" nolabel="1" colspan="4">
                                    <tree string="Extra Fields">
                                        <field name="type_id" string="Name"
                                            options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True}"/>
                                        <field name="value_extra" string="Value"/>
                                    </tree>
                                </field>
                            </group>
                        </page>
                        <page string="Factor Supplier">
                            <separator colspan="4" string="Waybill Supplier Factors"/>
                            <field name="supplier_factor_ids" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}" colspan="4"
                                context="{'default_category': 'supplier'}" nolabel="1"/>
                        </page>
                        <page string="Factor Driver">
                            <separator colspan="4" string="Driver Salary Factors"/>
                            <field name="driver_factor_ids" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}" colspan="4"
                                context="{'default_category': 'driver'}"  nolabel="1"/>
                        </page>
                        <page string="Times">
                            <group col="4" colspan="4">
                                <group colspan="2">
                                    <separator string="Scheduled"/>
                                    <newline/>
                                    <field name="date_start" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_up_start_sched" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_up_end_sched" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}" />
                                    <field name="date_up_docs_sched" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_appoint_down_sched" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_down_start_sched" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_down_end_sched" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_down_docs_sched" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_end"/>
                                </group>
                                <group colspan="2">
                                    <separator string="Real"/>
                                    <newline/>
                                    <field name="date_start_real" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_up_start_real" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_up_end_real" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_up_docs_real" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_appoint_down_real" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}" />
                                    <field attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}" name="date_down_start_real"/>
                                    <field attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}" name="date_down_end_real"/>
                                    <field attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}" name="date_down_docs_real"/>
                                    <field attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}" name="date_end_real"/>
                                </group>
                            </group>
                        </page>
                        <page string="Notes">
                            <field name="notes" attrs="{'readonly':[('state', 'in', ('confirmed', 'cancel'))]}" colspan="4" nolabel="1" placeholder="Notes..."/>
                        </page>
                        <page string="Log">
                            <group col="4" colspan="4">
                                <group colspan="2">
                                    <separator colspan="2" string="Client Invoice Data"/>
                                    <field name="invoice_id" readonly="False"
                                        options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True}"/>
                                    <field name="invoice_paid"/>
                                    <newline/>
                                </group>
                                <group colspan="2">
                                    <separator colspan="2" string="Supplier Invoice Data"/>
                                    <field name="supplier_invoice_id"
                                        options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True}"/>
                                    <field name="supplier_invoice_paid"/>
                                    <newline/>
                                </group>
                            </group>
                        </page>
                        <page string="Customs">
                            <separator coslpan="4" string="Customs"/>
                            <field name="custom_ids" nolabel="1"/>
                        </page>
                        <page string="Expenses">
                            <field name="expense_ids"/>
                        </page>
                    </notebook>
                </sheet>

                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers"/>
                    <field name="message_ids" placeholder="Share a message..." widget="mail_thread"/>
                </div>
            </form>
        </field>
    </record>

    <record id="tms_waybill_view_search" model="ir.ui.view">
        <field name="name">tms.waybill.search</field>
        <field name="model">tms.waybill</field>
        <field name="arch" type="xml">
            <search string="Search Waybills">
                <field name="name"/>
                <field name="partner_id"/>
                <field name="operating_unit_id"/>
                <field name="date_order" string="Waybill date"/>
                <field name="user_id"/>
                <separator/>
                <filter name="filter_state_draft" domain="[('state', '=', 'draft')]" icon="terp-gtk-media-pause" string="Pending"/>
                <filter name="filter_state_approved" domain="[('state', '=', 'approved')]" icon="terp-gtk-jump-to-ltr" string="Approved"/>
                <filter name="filter_state_confirmed" domain="[('state', '=', 'confirmed')]" icon="terp-check" string="Confirmed"/>
                <separator/>
                <filter name="filter_invoiced" domain="[('invoice_id', '!=', False)]" icon="terp-stock_symbol-selection" string="Invoiced"/>
                <filter name="filter_not_invoiced" domain="[('invoice_id', '=', False)]" icon="terp-dialog-close" string="Not Invoiced"/>
                <filter name="filter_state_not_cancel" domain="[('state', '!=', 'cancel')]" icon="terp-stock_symbol-selection" string="Not Cancelled"/>
                <filter name="filter_this_month" domain="[('date_order', '&gt;=', time.strftime('%Y-%m-01'))]" icon="terp-check" string="From this month"/>
                <newline/>
                <filter name="filter_user_id" domain="[('user_id', '=', uid)]" help="My Waybills"/>
                <newline/>
                <group expand="0" string="Group By...">
                    <filter name="group_by_partner_id" context="{'group_by': 'partner_id'}" string="Customer"/>
                    <filter name="group_by_state" context="{'group_by': 'state'}" string="State"/>
                    <filter name="group_by_date_order" context="{'group_by': 'date_order'}" string="Order Date"/>
                    <filter name="group_by_user_id" context="{'group_by': 'user_id'}" string="Salesman"/>
                    <filter name="group_by_date_order" context="{'group_by': 'date_order'}" string="Period"/>
                    <filter name="group_by_operating_unit_id" context="{'group_by': 'operating_unit_id'}" string="Operating Unit"/>
                </group>
            </search>
        </field>
    </record>

    <record id="tms_waybill_view_calendar" model="ir.ui.view">
        <field name="name">tms.waybill.calendar</field>
        <field name="model">tms.waybill</field>
        <field name="arch" type="xml">
            <calendar color="state" date_start="date_order" string="Waybills">
                <field name="partner_id"/>
                <field name="amount_untaxed"/>
            </calendar>
        </field>
    </record>

    <record id="tms_waybill_view_graph" model="ir.ui.view">
        <field name="name">tms.waybill.graph</field>
        <field name="model">tms.waybill</field>
        <field name="arch" type="xml">
            <graph string="Waybill" type="bar">
                <field name="partner_id"/>
                <field name="amount_untaxed" operator="+"/>
            </graph>
        </field>
    </record>

    <record id="tms_waybill_expense_view_form" model="ir.ui.view">
        <field name="name">tms.waybill.expense.form</field>
        <field name="model">tms.waybill</field>
        <field name="priority" eval="20"/>
        <field name="arch" type="xml">
            <form string="Waybill">
                <sheet>
                    <header>
                        <field name="state" statusbar_visible="draft,approved,confirmed" widget="statusbar"/>
                    </header>
                    <h1>
                        <field name="name" class="oe_inline" readonly="1"/>
                    </h1>
                    <group>
                        <group>
                            <field name="operating_unit_id" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"
                                options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True}"/>
                            <field name="currency_id" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}" groups="base.group_multi_currency"
                                options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True}"/>
                        </group>
                        <group>
                            <field attrs="{'readonly':[('state', 'in', ('confirmed', 'cancel'))]}" name="date_order"/>
                        </group>
                    </group>
                    <separator colspan="2" string="·  ·  ·  ·  ·  ·  ·  ·  ·  ·  ·  ·  ·  ·  ·  ·" style="text-align: center;"/>
                    <group colspan="2">
                        <field name="partner_id" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}" colspan="2"
                            domain="[('is_company', '=', True)]"
                            options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True}"/>
                    </group>
                    <group>
                        <group>
                            <field name="partner_order_id" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"
                                context="{'search_default_customer': 1, 'show_address': 1}"
                                domain="[('parent_id', '=', partner_id)]"
                                options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True}"/>
                            <field name="departure_address_id"  attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"
                                context="{'search_default_customer': 1, 'show_address': 1}"
                                options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True}"/>
                            <field name="upload_point" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                        </group>
                        <group>
                            <field name="partner_invoice_id"  attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"
                                context="{'search_default_customer': 1, 'show_address': 1}"
                                domain="[('parent_id', '=', partner_id)]"
                                options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True}"/>
                            <field name="arrival_address_id" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"
                                context="{'search_default_customer': 1, 'show_address': 1}"
                                options="{'no_create': True, 'no_create_edit': True, 'no_quick_create': True}"/>
                            <field name="download_point" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}" />
                        </group>
                    </group>
                    <separator/>
                    <notebook colspan="4">
                        <page string="Factor Driver">
                            <separator colspan="4" string="Driver Salary Factors"/>
                            <field colspan="4" context="{'default_category': 'driver'}" name="driver_factor_ids" nolabel="1"/>
                        </page>
                        <page string="Details">
                           <separator coslpan="4" string="Transportable"/>
                            <field attrs="{'readonly':[('state','in',('confirmed'))]}" colspan="4" name="transportable_line_ids" nolabel="1">
                                <tree editable="bottom">
                                    <field name="transportable_id"/>
                                    <field name="name"/>
                                    <field name="transportable_uom_id"/>
                                    <field name="quantity"/>
                                    <field name="notes"/>
                                </tree>
                            </field>
                            <separator colspan="4" string="Waybill Lines"/>
                            <field attrs="{'readonly':[('state','in',('confirmed'))]}" colspan="4" name="waybill_line_ids" nolabel="1">
                                <tree editable="bottom" string="Waybill Lines">
                                    <field name="product_id" domain="[('tms_product_category', 'in', ('freight', 'move', 'insurance', 'tolls', 'other'))]"/>
                                    <field name="name"/>
                                    <field name="product_qty"/>
                                    <field name="unit_price"/>
                                    <field name="discount"/>
                                    <field name="tax_ids" widget="many2many_tags"/>
                                    <field name="price_subtotal"/>
                                    <field invisible="1" name="tax_amount"/>
                                </tree>
                            </field>
                            <group col="4" colspan="4">
                                <group colspan="2">
                                    <separator string="Scheduled"/>
                                    <newline/>
                                    <field name="date_start" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_up_start_sched" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_up_end_sched" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}" />
                                    <field name="date_up_docs_sched" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_appoint_down_sched" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_down_start_sched" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_down_end_sched" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_down_docs_sched" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_end"/>
                                </group>
                                <group colspan="2">
                                    <separator string="Real"/>
                                    <newline/>
                                    <field name="date_start_real" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_up_start_real" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_up_end_real" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_up_docs_real" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}"/>
                                    <field name="date_appoint_down_real" attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}" />
                                    <field attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}" name="date_down_start_real"/>
                                    <field attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}" name="date_down_end_real"/>
                                    <field attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}" name="date_down_docs_real"/>
                                    <field attrs="{'readonly': [('state', 'in', ('confirmed', 'cancel'))]}" name="date_end_real"/>
                                </group>
                            </group>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <record id="tms_waybill_view_form_cust" model="ir.ui.view">
        <field name="name">tms.waybill.form.readonly.cust</field>
        <field name="model">tms.waybill</field>
        <field name="inherit_id" ref="view_tms_waybill_form"/>
        <!-- <field name="groups_id" eval="[(6, 0, [ref('tms.group_expenses')])]"/> -->
        <field name="arch" type="xml">
            <field name="driver_factor_ids" position="replace">
                <field name="driver_factor_ids" context="{'default_category': 'driver'}" />
            </field>
        </field>
    </record>

    <record id="tms_waybill_action" model="ir.actions.act_window">
        <field name="name">Waybills</field>
        <field name="res_model">tms.waybill</field>
        <field name="view_mode">tree,form,calendar,graph</field>
        <field name="context">{'search_default_user_id': uid, 'search_default_filter_this_month': 1}</field>
        <field name="help">Waybills</field>
    </record>

    <record id="tms_waybill_report_action" model="ir.actions.report">
        <field name="name">Waybill Report</field>
        <field name="model">tms.waybill</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">tms.waybill_report_template</field>
        <field name="report_file">Waybill Report</field>
