<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="tms_customs_view_tree" model="ir.ui.view">
        <field name="name">Customs</field>
        <field name="model">tms.customs</field>
        <field name="arch" type="xml">
            <tree string="Description" editable="bottom">
                <field name="customs"/>
                <field name="custom_house_id"/>
                <field name="datetime"/>
                <field name="return_datetime"/>
                <field name="returned_datetime"/>
            </tree>
        </field>
    </record>

    <record id="tms_customs_view_form" model="ir.ui.view">
        <field name="name">tms.custom.form</field>
        <field name="model">tms.customs</field>
        <field name="arch" type="xml">
            <form string="Description">
                <sheet>
                <group>
                    <field name="customs"/>
                    <field name="custom_house_id"/>
                    <field name="datetime"/>
                    <field name="return_datetime"/>
                    <field name="returned_datetime"/>
                </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="tms_customs_action" model="ir.actions.act_window">
        <field name="name">Customs</field>
        <field name="res_model">tms.customs</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>
