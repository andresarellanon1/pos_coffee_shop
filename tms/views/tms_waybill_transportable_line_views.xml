<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="tms_waybill_transportable_line_view_form" model="ir.ui.view">
        <field name="name">tms.waybill.transportable.line.form</field>
        <field name="model">tms.waybill.transportable.line</field>
        <field name="arch" type="xml">
            <form string="Waybill Transportable Line">
                <sheet>
                    <group>
                        <group>
                            <field name="sequence"/>
                            <field name="transportable_uom_id"/>
                            <field name="transportable_id"/>
                            <field name="waybill_id"/>
                            <field name="name"/>
                        </group>
                        <group>
                            <field name="notes"/>
                            <field name="quantity"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
