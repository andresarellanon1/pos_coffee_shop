<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="custom_product_screen.ProductSpawnerActionpad" 
        owl="1" >
        <div>
            <button class="p-3 mb-2 bg-danger text-white" t-on-click="() => this.trigger('close-temp-screen')">
               Cancelar 
            </button>
            <button class="p-3 mb-2 bg-success text-white" t-on-click="() => this.trigger('spawn-product')">
                Aceptar
            </button>
        </div>
   </t>
</templates>
