<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
	<t t-name="custom_product_screen.ProductTemplateWidget" owl="1">
		<div class="container-fluid">
			<div class="row row-cols-6 grid gap-2 pt-2">
				<t t-foreach="productsTemplateToDisplay" t-as="product" t-key="product.id">
