<odoo>
    <record id="q_endpoint_request_body_list_view" model="ir.ui.view">
        <field name="name">q_endpoint_request_body.list</field>
        <field name="model">q_endpoint_catalog.request_body</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="value" />
                <field name="endpoint_id" invisible="1" />
            </tree>
        </field>
    </record>

    <record id="q_endpoint_request_body_form_view" model="ir.ui.view">
        <field name="name">q_endpoint_request_body.form</field>
        <field name="model">q_endpoint_catalog.request_body</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name" />
                    <field name="value" />
                    <field name="endpoint_id" invisible="1" />
                </group>
            </form>
        </field>
    </record>

    <record id="action_q_endpoint_request_body_list" model="ir.actions.act_window">
        <field name="name">Request Body</field>
        <field name="res_model">q_endpoint_catalog.request_body</field>
        <field name="view_mode">tree,kanban,form</field>
    </record>
</odoo>
