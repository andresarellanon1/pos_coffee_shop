<odoo>
    <record id="q_endpoint_headers_list_view" model="ir.ui.view">
        <field name="name">q_endpoint_headers.list</field>
        <field name="model">q_endpoint_catalog.headers</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="value" />
                <field name="endpoint_id" invisible="1" />
            </tree>
        </field>
    </record>

    <record id="q_endpoint_headers_form_view" model="ir.ui.view">
        <field name="name">q_endpoint_headers.form</field>
        <field name="model">q_endpoint_catalog.headers</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name" />
                    <field name="value" />
                    <field name="endpoint_id" invisible="1" />
                </group>
            </form>
        </field>
    </record>

    <record id="action_q_endpoint_headers_list" model="ir.actions.act_window">
        <field name="name">Headers</field>
        <field name="res_model">q_endpoint_catalog.headers</field>
        <field name="view_mode">tree,kanban</field>
    </record>
</odoo>
