<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
<t t-name="tms_customer_waybill.CustomerWaybillWidget" owl="1">
    <table class="border p-2 flex flex-col">
        <thead>
            <tr>
                <th t-foreach="_headers" t-as="header" t-key="header">
                    <t t-esc="header"/>
                </th>
                <th t-foreach="_actions" t-as="action" t-key="action">
                    <t t-esc="action"/>
                </th>
            </tr>
        </thead>
        <tbody>
            <tr t-foreach="_items" t-as="item" t-key="item.id">
                <td t-foreach="Object.keys(item)" t-as="key" t-key="key">
                    <t t-esc="item[key]"/>
                </td> 
                <td t-foreach="_actions" t-as="action" t-key="action">
                    <button type="button" t-on-click="() => this.actionCall(action)">
                        <t t-esc="action"/>
                    </button>
                </td>
            </tr>
        </tbody>
    </table>
</t>
</templates>
