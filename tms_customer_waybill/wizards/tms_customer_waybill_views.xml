<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="tms_customer_waybill_wizard_view" model="ir.ui.view">
        <field name="name">custom.wizard.form</field>
        <field name="model">tms_customer_waybill.customer_waybill_wizard</field>
        <field name="arch" type="xml">
            <form string="Customer">
                <group>
                    <field name="contact" required="1" />
                    <field name="endpoint" required="1" />
                </group>
                <group string="Customer Waybills">
                    <field name="remote_waybills" widget="customer_waybill_widget" />
                </group>
                <footer>
                    <button string="Cancel" class="btn-secondary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
</odoo>

